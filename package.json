{
  "name": "app",
  "version": "1.0.0",
  "scripts": {
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint . --ext .jsx,.js,.tsx,.ts",
    "lint:style": "npx stylelint \"**/*.scss\"",
    "webpack:dev": "cross-env NODE_ENV=development webpack serve --config config/webpack/webpack.dev.js",
    "webpack:prod": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.prod.js",
    "dev": "npm run webpack:dev",
    "build": "run-s lint webpack:prod",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": [
    "defaults",
    "not dead",
    "> 0.01%"
  ],
  "devDependencies": {
    "@anton.bobrov/eslint-config": "^1.4.0",
    "@types/three": "^0.159.0",
    "autoprefixer": "^10.4.15",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.53.0",
    "gh-pages": "^5.0.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "raw-loader": "^4.0.2",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "stylelint": "^14.11.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "ts-loader": "^9.4.4",
    "typescript": "5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@anton.bobrov/react-dat-gui": "^3.0.4",
    "@anton.bobrov/vevet-init": "^3.11.0",
    "normalize.css": "^8.0.1",
    "three": "^0.159.0",
    "ts-essentials": "^9.4.0",
    "vevet-dom": "^0.0.15"
  }
}
